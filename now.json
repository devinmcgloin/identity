{
  "version": 2,
  "name": "identity",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "build": {
    "env": {
      "GA_ANALYTICS_ID": "@ga-analytics-id",
      "UNSPLASH_SECRET_KEY": "@unsplash-secret-key",
      "UNSPLASH_ACCESS_KEY": "@unsplash-access-key"
    }
  }
}
